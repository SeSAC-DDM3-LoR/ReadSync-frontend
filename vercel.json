{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://readsync.kro.kr/api/:path*"
        },
        {
            "source": "/actuator/:path*",
            "destination": "https://readsync.kro.kr/actuator/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}