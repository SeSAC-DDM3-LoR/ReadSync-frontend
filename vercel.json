{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "http://readsync-server-env.eba-auskxygg.ap-northeast-2.elasticbeanstalk.com/api/:path*"
        },
        {
            "source": "/actuator/:path*",
            "destination": "http://readsync-server-env.eba-auskxygg.ap-northeast-2.elasticbeanstalk.com/actuator/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}